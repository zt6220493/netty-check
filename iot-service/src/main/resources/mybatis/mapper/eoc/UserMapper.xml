<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ecar.eoc.mapper.eocdb.user.UserMapper" >
    <resultMap id="BaseResultMap" type="com.ecar.eoc.entity.user.User" >
        <id column="USER_ID" property="id" jdbcType="INTEGER" />
        <result column="USER_NAME" property="userName" jdbcType="VARCHAR" />
        <result column="LOGIN_NAME" property="loginName" jdbcType="VARCHAR" />
        <result column="PASSWORD" property="passWord" jdbcType="VARCHAR"/>
        <result column="CREATE_TIME" property="createTime" jdbcType="DATE" />
    </resultMap>
    
    <sql id="Base_Column_List" >
        USER_ID, USER_NAME, LOGIN_NAME, PASSWORD, CREATE_TIME
    </sql>

    <select id="getAll" resultMap="BaseResultMap" >
       SELECT 
       <include refid="Base_Column_List" />
	   FROM EOC_PLT_USER WHERE USER_NAME IS NOT NULL
    </select>
</mapper>